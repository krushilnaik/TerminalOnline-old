{
	"compilerOptions": {
		"target": "es2018",
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"checkJs": true,
		"jsx": "react",
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true
	},
	"exclude": ["node_modules", "**/build"]
}
